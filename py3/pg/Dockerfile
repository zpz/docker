FROM zppz/py3:20170313

USER root
WORKDIR /

RUN apt-get update \
    && apt-get install -y --no-install-recommends \
        libpq5 \
        libpq-dev \
    && pip install --no-cache-dir --upgrade \
        'psycopg2==2.7.1' \
    && rm -rf /var/lib/apt/lists/* /tmp/* \
    && apt-get -y autoremove \
    && apt-get clean

RUN pip install --no-cache-dir --upgrade \
        'aiohttp==1.3.5' \
        'asyncpg==0.9.0' \
        'uvloop==0.8.0'

CMD ["/bin/bash"]
